{
    "name": "peridot-php/event-emitter",
    "description": "EventEmitter is a very simple event dispatching library for PHP",
    "keywords": ["event-dispatcher", "event-emitter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        },
        {
            "name": "Brian Scaturro",
            "email": "scaturrob@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Peridot\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "peridot-php/peridot":"2.0.*@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
